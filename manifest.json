{
  "manifest_version": 3,
  "name": "Meta Tag Fetcher Pro",
  "version": "1.0.0",
  "version_name": "1.0.0 Beta",
  "description": "A powerful tool for extracting, managing and customizing meta tags from web pages. Perfect for developers, SEO specialists and content managers.",
  "author": "Zongxiao Cheng",
  "homepage_url": "https://github.com/zongxiaocheng/meta-tag-fetcher",
  "support_url": "https://github.com/zongxiaocheng/meta-tag-fetcher/issues",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "clipboardWrite",
    "history"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Meta Tag Fetcher Pro"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "options_page": "options.html",
  "minimum_chrome_version": "88",
  "offline_enabled": true,
  "short_name": "MetaFetcher",
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "Command+Shift+F"
      },
      "description": "Open Meta Tag Fetcher"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
